{
  "manifest_version": 3,
  "name": "Swello Replit Theme",
  "version": "0.1.0",
  "description": "Swello Replit Theme by Sam Cheng",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://replit.com/*"]
  }],
 "action":{
    "default_popup": "index.html",
    "default_title": "Swello Replit Theme"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}